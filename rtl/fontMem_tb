    .scope S_0x56287e204260;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x56287e224b30_0, 0, 1;
    %pushi/vec4 0, 0, 7;
    %store/vec4 v0x56287e224a40_0, 0, 7;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x56287e224d00_0, 0, 1;
    %vpi_call 2 23 "$readmemb", "charmem2.list", v0x56287e224da0 {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x56287e204260;
T_4 ;
    %wait E_0x56287e1fd440;
    %delay 1, 0;
    %load/vec4 v0x56287e224c00_0;
    %load/vec4 v0x56287e224a40_0;
    %pad/u 9;
    %ix/vec4 4;
    %load/vec4a v0x56287e224da0, 4;
