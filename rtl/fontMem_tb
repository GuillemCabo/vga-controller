E_0x56287e1fd440 .event posedge, v0x56287e224750_0;
    .scope S_0x56287e2043e0;
T_0 ;
    %vpi_call 3 18 "$readmemb", P_0x56287e204630, v0x56287e224900 {0 0 0};
    %end;
    .thread T_0;
    .scope S_0x56287e2043e0;
T_1 ;
    %wait E_0x56287e1fd440;
    %load/vec4 v0x56287e203780_0;
    %pad/u 9;
    %ix/vec4 4;
    %load/vec4a v0x56287e224900, 4;
    %assign/vec4 v0x56287e224810_0, 0;
    %jmp T_1;
    .thread T_1;
    .scope S_0x56287e204260;
T_2 ;
    %vpi_call 2 12 "$dumpfile", "fontMem_tb.vcd" {0 0 0};
    %vpi_call 2 13 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x56287e204260 {0 0 0};
T_2.0 ;
    %load/vec4 v0x56287e224a40_0;
    %pushi/vec4 127, 0, 7;
    %cmp/e;
    %flag_get/vec4 4;
    %cmpi/ne 1, 0, 1;
    %jmp/0xz T_2.1, 6;
    %wait E_0x56287e1fd970;
    %jmp T_2.0;
T_2.1 ;
    %load/vec4 v0x56287e224d00_0;
    %cmpi/e 0, 0, 1;
    %jmp/0xz  T_2.2, 4;
    %vpi_call 2 15 "$display", "PASS" {0 0 0};
T_2.2 ;
    %vpi_call 2 16 "$finish" {0 0 0};
    %end;
